<rect {{xywh 0 0 .w .h}} {{fl_stk "white" "black" (mul .w 0.02) }} />
<text {{xy (mul .w 0.1) (mul .h 0.2)}} {{font (mul .w 0.1) "Arial"}}> {{.name}}</text>

{{$gapx := div .w 8.0}}
{{$gapy := div .h 10.0 }}
{{$sx := div .w 7.0}}
{{$sy := mul .h 0.3}}
{{$ft := font (mul .w 0.05) "Arial"}}

{{range $kx,$vx := .data.dat }}
    {{range $ky, $vy := $vx }}
        <text {{xy (add $sx (mul $kx $gapx)) (add $sy (mul $ky $gapy)) }}
            {{$ft}} >{{$vy}}</text>
    {{end}}
{{end}}

